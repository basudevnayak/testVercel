// }
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
// Address is an embedded document
// type Address {
//   street String
//   city   String
//   state  String
//   zip    String
// }

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String   @unique
  name      String?
  // address   Json? 
  create_at DateTime @default(now())
}

