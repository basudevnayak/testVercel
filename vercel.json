{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**/*.json"],
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "Cache-Control": "s-maxage=60"
      }
    }
  ]
}